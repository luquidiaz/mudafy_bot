{
  "name": "mudafy-bot",
  "version": "1.0.0",
  "description": "WhatsApp AI Assistant with BuilderBot + OpenAI",
  "main": "dist/app.js",
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "npm run lint && nodemon --exec tsx src/app.ts",
    "build": "npx rollup -c",
    "start": "node ./dist/app.js",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "create-market-data-agent": "tsx scripts/create-market-data-agent.ts",
    "update-market-data": "tsx scripts/update-market-data.ts",
    "update-orchestrator-market": "tsx scripts/update-orchestrator-with-market.ts"
  },
  "keywords": [
    "chatbot",
    "whatsapp",
    "builderbot",
    "openai",
    "ai",
    "assistant"
  ],
  "author": "Lucas Diaz",
  "license": "MIT",
  "dependencies": {
    "@builderbot/bot": "^1.3.5",
    "@builderbot/provider-baileys": "^1.3.5",
    "dotenv": "^16.4.5",
    "openai": "^4.28.0",
    "qrcode": "^1.5.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "@types/qrcode": "^1.5.6",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.4.0",
    "eslint": "^8.52.0",
    "eslint-plugin-builderbot": "latest",
    "nodemon": "^3.1.0",
    "rollup": "^4.10.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "tsx": "^4.7.1",
    "typescript": "^5.4.3"
  }
}
